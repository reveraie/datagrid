{
    "compilerOptions": {
      "target": "ES5",
      "lib": ["es2018", "dom"],
      "module": "ES6",
      "declaration": true,
      "outDir": "dist",
      "jsx": "react",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "**/__tests__/*"]
  }
  